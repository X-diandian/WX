/* components/date/date.wxss */
.navErapLeft {
  display: flex;
}
.likeFont {
  font-size: 18rpx;
  font-weight: 400;
  color: rgba(187, 187, 187, 1);
}
.texts {
  font-size: 32rpx;
  color: rgba(0, 0, 0, 1);
  line-height: 100rpx;
}
.navErapLeftBig {
  font-size: 58rpx;
  font-weight: bold;
  color: rgba(0, 0, 0, 1);
  line-height: 100rpx;
}
.navErapLeftLandscape {
  display: flex;
  flex-direction: column;
  margin-left: 14rpx;
  height: 42rpx;
  margin-top: 29rpx;
  border-left: 2rpx solid rgba(0, 0, 0, 1);
  padding-left: 16rpx;
}

.navErapLeftLandscape text:nth-child(1) {
  font-size: 24rpx;
  font-weight: 500;
  color: rgba(0, 0, 0, 1);
  height: 24rpx;
  line-height: 24rpx;
}

.navErapLeftLandscape text:nth-child(2) {
  font-size: 20rpx;
  height: 20rpx;
  line-height: 20rpx;
  font-weight: 500;
  color: rgba(0, 0, 0, 1);
}